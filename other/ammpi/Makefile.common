# variables added here should also go in Makefile.titanium.in, and vice-versa

srcdir = .

# compiler
CC := mpicc
ccdebugflags := -O0 -g3 -ansi -Wall -Wno-unused-variable -Wno-unused-function
ccoptflags := -O3

# system utilities
ranlib = ranlib
ar = ar

# To be set by pre-baked makefiles:
endianness := 
platform_defines := 
platform_libraries := 
